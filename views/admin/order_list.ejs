<%-include('nav.ejs')%>

<div class="container-fluid bg-white p-3">
    <div class="row">
        <div class="col-12">
            <h2 class="p-4 text-center"><b>Order List</b></h2>
                <div class="col-md-12">
                    <table class="table table-sm table-bordered">
                        <tr class="bg-success">
                            <th>Print</th>
                            <th>SN</th>
                            <th>Table No</th>
                            <th>Date</th>
                            <th>Time</th>
                            <th>Order Amount</th>
                            <th>Paid</th>
                            <th>Pending</th>
                        </tr>
                        <%
                        for(i=0;i<data.length;i++)
                        {
                        %>
                        <tr>
                            <th>
                                <a href="/admin/printbill/<%=data[i].order_tbl_id%>">
                                    <button class="btn btn-dark btn-sm"><i class="fa fa-print"></i></button>
                                </a>   
                            </th>
                            <th><%=i+1%></th>
                            <th><%=data[i].table_name%></th>
                            <th><%=data[i].order_date%></th>
                            <th><%=data[i].entry_time%> - <%=data[i].exit_time%></th>
                            <th> &#8377; <%=data[i].order_amt%> /-</th>
                            <th> &#8377; <%=data[i].paid_amt%> /-</th>
                            <th><%=data[i].pending_amt%> /-</th>
                        </tr>
                        <%
                        }
                        %>
                    </table>
                </div>
        </div>
    </div>
</div>


<%-include('footer.ejs')%>